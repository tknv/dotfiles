echo -e "\033[2;38;5;44mset android build env\033[30m"
export CROSS_COMPILE=/usr/bin/arm-linux-gnueabi-
echo -e "\033[1;38;5;11mCROSS COMPILER PATH =\033[30m \033[1;38;5;41m$CROSS_COMPILE\033[30m "
export CFLAGS="$CFLAGS -Wno-error=unused-but-set-variable -Wno-error=array-bounds"
echo -e "\033[1;38;5;11mCFLAGS \033[30m \033[1;38;5;41m$CFLAGS\033[30m "
export ARCH=arm
echo -e "\033[1;38;5;11mARCH = \033[30m \033[1;38;5;41m$ARCH \033[30m"
cd ${HOME}/android
tput sgr0

